{
  "name": "aniportable",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.8.10",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.3.5",
    "chart.js": "^4.5.0",
    "graphql": "^16.11.0",
    "lucide-react": "^0.526.0",
    "plasmo": "^0.90.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "recharts": "^3.1.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.1",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.8.3"
  },
  "resolutions": {
    "@apollo/client": "3.8.10"
  },
  "overrides": {
    "@apollo/client": "3.8.10"
  },
  "manifest": {
    "manifest_version": 3,
    "name": "AniPortable",
    "version": "1.0.0",
    "description": "Track and view your AniList anime without leaving your current tab!",
    "permissions": [
      "storage",
      "identity"
    ],
    "host_permissions": [
      "https://anilist.co/*",
      "https://graphql.anilist.co/*"
    ],
    "background": {
      "service_worker": "background.ts",
      "type": "module"
    }
  }
}