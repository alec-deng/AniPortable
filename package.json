{
  "name": "anilist-recap",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.3.5",
    "chart.js": "^4.5.0",
    "graphql": "^16.11.0",
    "lucide-react": "^0.526.0",
    "plasmo": "^0.90.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^3.1.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.8.3"
  },
  "manifest": {
    "manifest_version": 3,
    "name": "AniList Recap",
    "version": "1.0.0",
    "description": "Analyze your AniList anime data with graphs!",
    "permissions": [
      "storage",
      "identity"
    ],
    "host_permissions": [
      "https://anilist.co/*",
      "https://graphql.anilist.co/*"
    ],
    "background": {
      "service_worker": "background.ts",
      "type": "module"
    }
  }
}
